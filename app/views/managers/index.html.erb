<h1>Managers</h1>

<p><%= link_to "Add a Manager", new_manager_path%></p>

<ul>
	<% @managers.each do |manager| %>
		<li><%= manager.user_name %> - <%= link_to "Show", manager_path(manager) %>, <%= link_to "Edit", edit_manager_path(manager) %>,  <%= link_to "Delete", manager_path(manager), method: :delete, confirm: "Are you sure?" %></li>
	<%end%>
</ul>
